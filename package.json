{
  "name": "schiphol",
  "version": "0.0.1",
  "description": "A wrapper around the Schiphol API.",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "author": "Wigger Boelens <me@wiggerboelens.com> (http://wiggerboelens.com/)",
  "dependencies": {
    "limits.js": "^0.1.4",
    "nock": "^9.0.9",
    "node-fetch": "^1.6.3",
    "rimraf": "^2.6.1",
    "ts-jest": "^19.0.4"
  },
  "peerDependencies": {
    "typescript": "^2.1.0 || ^2.2.0-dev"
  },
  "devDependencies": {
    "@types/jest": "latest",
    "@types/node": "7.0.8",
    "jest": "^19.0.2",
    "run-sequence": "^1.2.2",
    "tslint": "^4.5.1"
  },
  "scripts": {
    "compile": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint ./src/**/*.ts",
    "clean": "rimraf lib",
    "prepublish": "run-s lint clean compile"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/__tests__/.*\\.(test)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
